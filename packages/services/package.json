{
  "name": "@elkdonis/services",
  "version": "1.0.0",
  "description": "Backend services and business logic for meetings, posts, and integrations",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@elkdonis/db": "workspace:*",
    "@elkdonis/nextcloud": "workspace:*",
    "@elkdonis/types": "workspace:*",
    "@elkdonis/utils": "workspace:*",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.17",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
